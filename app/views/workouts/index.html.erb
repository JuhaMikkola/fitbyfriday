<% if current_user %>

  <div class="notifications">
      <%= render 'rating_requests' %>
      <%= render 'invites' %>
      <%= render 'friends' %>
  </div>

  <div id="workout-container">
    <%= render 'workouts' %>
  </div>


  <div id="new-workout-container" class="reveal-modal" data-reveal>
    <%= render 'new_workout' %>
  </div>

  <div class="row add-workout">
    <div class="small-1 column">
      <a href="#" data-reveal-id="new-workout-container"><i class="fi-plus"></i></a>
    </div>
    <div class="small-9 column add-text">
      <h3>Add New Workout</h3>
    </div>
  </div>



<% else %>

  <h2><%= link_to "Sign Up", new_user_path  %> or <%= link_to "Login", login_path %></h2>

<% end %>




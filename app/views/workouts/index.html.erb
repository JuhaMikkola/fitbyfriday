
<div class="content-primary">

<% if current_user %>
  <a href="#" data-reveal-id="new_workout" class="button">New Workout</a>
  <%= render 'workouts' %>
  <%= render 'invites' %>
  <%= render 'rating_requests' %>
  <%= render 'friends' %>

  <div id="new_workout" class="reveal-modal" data-reveal>
  <%= render 'new_workout' %>
  </div>

<% else %>

  <h2><%= link_to "Sign Up", new_user_path  %> or <%= link_to "Login", login_path %></h2>

<% end %>




  

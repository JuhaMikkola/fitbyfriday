

<%= form_tag user_sessions_path, :method => :post do %>

<fieldset>
  <div class="#">

    <%= label_tag :email %>
    <%= text_field_tag :email %>
  </div>

  <div class="#">
    <%= label_tag :password %>
    <%= password_field_tag :password %>
  </div>


  <div class="actions">
    <%= submit_tag "Login", class: "button"%>
  </div>

</fieldset>

<% end %>